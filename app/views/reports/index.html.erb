<table class="table">
  <tr>
    <td class="col-md-2"><h3 class="title"><span class="fa fa-area-chart"></span> Reports</h3></td>
    <td class="col-md-10">
      <div class="msg">
        <% if flash[:alert].present? %>
          <div class="alert alert-danger"><%= flash[:alert] %></div>
        <% end %>
        <% if flash[:notice].present? %>
          <div class="alert alert-success"><%= flash[:notice] %></div>
        <% end %>
      </div>
    </td>
  </tr>
</table>

<table class="table table-striped table-bordered">
    <tr>
      <td class="col-md-2"><label>Name</label></td>
    </tr>
</table>

<table class="table table-striped table-bordered">
    <thead>
    <tr>
      <th class="col-md-2">Name</th>
      <th class="col-md-1">Amount</th>
      <th class="col-md-2">Date</th>
      <th class="col-md-2">Payment source</th>
      <th class="col-md-2">Member</th>
      <th class="col-md-2">Category</th>
      <th class="col-md-3">Action</th>
    </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <a href="#" class="fa fa-pencil-square-o edit" title="disabled"></a>
          <a href="#" class="fa fa-trash-o delete" title="disabled"></a>
        </td>
      </tr>
    </tbody>
</table>

